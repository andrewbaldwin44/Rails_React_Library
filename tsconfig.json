{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": true,
    "baseUrl": "app/javascript",
    "paths": {
      "*": ["types/*", "*"]
    }
  },
  "include": ["app/javascript/**/*.ts", "app/javascript/**/*.tsx"],
  "exclude": ["node_modules"]
}
